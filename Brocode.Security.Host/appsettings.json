{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "GitHubApi": {
    "BaseUrl": "https://api.github.com",
    "AuthorizationToken": "bearer <YOUR_GITHUB_TOKEN>",
    "TimeoutInSeconds": 10,
    "RetryOptions": {
      "Retry": {
          "BackoffType": "Linear",
          "UseJitter": false,
          "MaxRetryAttempts": 7
      }
    }
  }
}
